import { Shipment } from "./Shipment";

export class DoNotLeaveDecorator implements Shipment {
    shipmentInstance: Shipment;
    constructor(instance: Shipment) {
        this.shipmentInstance = instance;
    }
    ship(): string {
        return this.shipmentInstance.ship() + "**MARK DO NOT LEAVE IF ADDRESS NOT AT HOME**";
    }

}